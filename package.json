{
  "name": "swcombine-sdk",
  "version": "2.0.1",
  "description": "Comprehensive TypeScript SDK for Star Wars Combine API v2.0",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:types": "tsc -p tsconfig.types.json",
    "build:watch": "tsc -p tsconfig.esm.json --watch",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:integration:api": "vitest run --config vitest.integration.config.ts tests/integration/api.test.ts",
    "test:integration:character": "vitest run --config vitest.integration.config.ts tests/integration/character.test.ts tests/integration/character-me.test.ts",
    "test:integration:faction": "vitest run --config vitest.integration.config.ts tests/integration/faction.test.ts",
    "test:integration:galaxy": "vitest run --config vitest.integration.config.ts tests/integration/galaxy.test.ts",
    "test:integration:market": "vitest run --config vitest.integration.config.ts tests/integration/market.test.ts",
    "test:integration:news": "vitest run --config vitest.integration.config.ts tests/integration/news.test.ts",
    "test:integration:types": "vitest run --config vitest.integration.config.ts tests/integration/types.test.ts",
    "test:integration:misc": "vitest run --config vitest.integration.config.ts tests/integration/misc.test.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "docs:api": "typedoc",
    "docs:api:watch": "typedoc --watch",
    "docs:html": "typedoc --options typedoc.html.json",
    "get-token": "tsx scripts/get-oauth-token.ts",
    "get-character-uid": "tsx scripts/get-character-uid.ts",
    "refresh-token": "tsx scripts/refresh-token.ts",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "swcombine",
    "star-wars",
    "api",
    "sdk",
    "oauth2",
    "typescript"
  ],
  "author": "Dreks Selmur <jonmarkgo@gmail.com>",
  "contributors": [
    {
      "name": "swc-marc"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jonmarkgo/swcombine-sdk-nodejs.git"
  },
  "bugs": {
    "url": "https://github.com/jonmarkgo/swcombine-sdk-nodejs/issues"
  },
  "homepage": "https://jonmarkgo.github.io/swcombine-sdk-nodejs/",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "^1.7.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/node": "^20.19.25",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "dotenv": "^17.2.3",
    "eslint": "^8.57.0",
    "express": "^5.1.0",
    "prettier": "^3.3.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typedoc": "^0.28.17",
    "typedoc-plugin-llms-txt": "^0.1.2",
    "typedoc-plugin-markdown": "^4.10.0",
    "typescript": "^5.5.0",
    "vitest": "^1.6.0"
  }
}
